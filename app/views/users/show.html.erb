<%= stylesheet_link_tag 'user', :media => "all" %>
<div class="user">

 <div class="top-box">
    <h3>マイページ</h3>
 </div>

 <p>名前 : <%= @user.username %></p>
 <p>メールアドレス : <%= @user.email %></p>
 <p>プロフィール : <%= @user.profile %></p>

 <% if current_user.id == @user.id %>
  <%= link_to "編集する", edit_user_registration_path %>
 <% end %>

 <%= link_to "分析一覧に戻る", corporations_path %>
</div>